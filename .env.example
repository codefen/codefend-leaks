# API Configuration
VITE_API_URI=https://api.codefend.com/index.php

# Stripe Payment Keys (for subscription management)
VITE_STRIPE_KEY_LIVE=pk_live_your_stripe_live_key_here
VITE_STRIPE_KEY_TEST=pk_test_your_stripe_test_key_here

# Google OAuth (Optional - for Google Sign-In)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_ENABLE_GOOGLE_AUTH=true

# Environment Settings
VITE_NODE_ENV=development
VITE_APP_ENV=development
VITE_DEBUG=true

# Tauri Desktop Signing (for desktop builds)
TAURI_SIGNING_PRIVATE_KEY="your_signing_private_key_here"
TAURI_SIGNING_PRIVATE_KEY_PASSWORD=your_signing_password_here
